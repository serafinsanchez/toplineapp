{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/extract/route.ts": {
      "memory": 3008,
      "maxDuration": 300
    },
    "app/api/process-start/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/api/process-status/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
} 